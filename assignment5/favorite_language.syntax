"(?<!\w)(\d+)(?!\w)": number
"\b(?:try|catch|finally)\b": try
"\b(?:true|false|null)\b": special
"@\w+": decorator
"\/\/[^\n]*": comment
"(?:\"\"\".+?(?:\"\"\"|\Z)|"[^"\n]*"|'[^'\n]*')": string
"(int\b|boolean\b|String\b|double\b|float\b|void\b)": types
"([\w][\w-]*\s*)(?=\()": function
"(\w*)(?=\ =|\.)": variable
"(static\b|public\b|private\b|protected\b)": access
"(?<!\w)(\d+)(?!\w)": number
"[a-zA-Z_]\w*(?=\()": function
"\b(?:try|except|finally)\b": try
"\b(?:import|from|as)\b": imports
"\b(?:True|False|None)\b": special
"@\w+": decorator
"#[^\n]*": comment
"(?:\"\"\".+?(?:\"\"\"|\Z)|\"\w*\"|'[^'\n]*')": string
"((?:\b)class(?:\b))": class
"\b(?:if|else|elif)\b": statements
"\b(?:def|in)\b": defin
"\b(?:while|for)\b": loops
"\b(?:return)\b": return
